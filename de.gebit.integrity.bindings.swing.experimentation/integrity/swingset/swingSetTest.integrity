import swing.launch.*
import swing.basic.*

packagedef swing.swingset with

	suitedef main with
	
		call launchApp mainClass: "SwingSet2"
		
		call clickButton name: "frame0.JToggleButton#13"
		
		test sliderMinValue name: "frame0.JSlider#0" = 0
		test sliderMaxValue name: "frame0.JSlider#0" = 10
		
		call setSliderValue name: "frame0.JSlider#0" value: 5
		test sliderValue name: "frame0.JSlider#0" = 5
		
		call swing.tools.authorAssist
		
		
		call killApp
	
	suiteend

packageend